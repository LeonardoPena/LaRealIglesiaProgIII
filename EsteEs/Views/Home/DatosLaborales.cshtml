@model EsteEs.Models.DatosLaborales
@{
    ViewData["Title"] = "Home Page";
    int? integrante_id = ViewBag.id_integrante;
}
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

    <title>Datos Laborales</title>
</head>

<div class="text-center">
    <h1 class="display-4">Datos Laborales</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>



<form method="post" asp-action="DatosLaborales">
    <input hidden value="@integrante_id" asp-for="IdIntegrante" />
    <div class="form-group">
        <label for="exampleInputEmail1">Profesion Actual</label>
        <input asp-for="Profesion" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Profesion">

    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">Empresa en la que trabaja</label>
        <input asp-for="NombreEmpresa" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Empresa">

    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">Ocupacion en la empresa</label>
        <input asp-for="Ocupacion" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Ocupacion">

    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">Telefono de la empresa</label>
        <input asp-for="Telefono" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Telefono empresarial">

    </div>

    <input type="submit" class="btn btn-primary" value="Continuar">

</form>